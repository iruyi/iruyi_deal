{
    "errcode":${errcode},
    "errmessage":"${errmessage}",
    "data":{
        [#if data?? && data?size>0]
            "viewList":[
                [#list data as a]
                    {
                      "id":${a.id},
                      "title":"${a.title!!}",
                      "content":"${a.content}",
                      "createDate":"${a.createDate?datetime}",
                      "isLive": ${(a.isLive)!0},
                      "lawyerId":${a.lawyerId},
                      "lawyerName":"${a.lawyerName!!}",
                      "photoUrl":"${a.photoUrl!!}",
                      "isAtten":${(a.isAtten)!0},
                      "viewTime":"${a.viewTime?datetime}",
                      "endTime":"${a.endTime?datetime}"
                    }
                    [#if a_has_next],[/#if]
                [/#list]
                   ]
        [#else]
            "viewList":[]
        [/#if]
    }
}