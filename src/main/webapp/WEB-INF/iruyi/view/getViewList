                  "isLive":${a.isLive},
                  "lawyerId":${a.lawyerId},
                  "lawyerName":"${a.lawyerName}",
                  "photoUrl":"${a.photoUrl!!}",
                  "isAtten":${(a.isAtten)!0}
                }
                [#if a_has_next],[/#if]
            [/#list]
               ]
    [#else]
        "data":[]
    [/#if]
}