    [/#if]
    [#if data.topicVoList?? && data.topicVoList?size>0]
        "data":[
            [#list data.topicVoList as a]
                {
                  "id":${a.id},
                  "content":"${a.content}",
                  "createDate":"${a.createDate?datetime}",
                  "lawyerId":${a.lawyerId},
                  "lawyerName":"${a.lawyerName}",
