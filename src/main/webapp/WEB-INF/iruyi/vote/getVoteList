{
    "errcode":${errcode},
    "errmessage":"${errmessage}",
    "data":{
        [#if data?? && data?size>0]
            "voteList":[
                [#list data as a]
                {
                        "voteDetail":{
                            "id":${a.id},
                            "title":"${a.title}",
                            "content":"${a.content}",
                            "createDate":"${a.createDate?datetime}",
                            "isEnd":${a.isEnd},
                            "isVote":${a.isVote!0}
                        },
                      [#assign voteOptionVos=a.voteOptionVos]
                      [#if voteOptionVos?? && voteOptionVos?size>0]
                        "voteOptions":[
                             [#list voteOptionVos as b]
                                {   "id":${b.id},
                                    "content":"${b.content}"
                                }
                                [#if b_has_next],[/#if]
                             [/#list]
                                        ]
                      [#else]
                        "voteOptionVos":[]
                      [/#if]
                }
                    [#if a_has_next],[/#if]
                [/#list]
                   ]
        [#else]
            "voteList":[]
        [/#if]
    }
}